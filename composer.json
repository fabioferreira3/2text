{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-redis": "*",
        "cloudinary-labs/cloudinary-laravel": "^2.0",
        "doctrine/dbal": "^3.6",
        "georgehadjisavva/elevenlabs-api-client": "^1.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/cashier": "^14.12",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.14",
        "laravel/jetstream": "^2.16",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.11",
        "norkunas/youtube-dl-php": "dev-master",
        "openai-php/laravel": "^0.7",
        "pusher/pusher-php-server": "^7.2",
        "rappasoft/laravel-livewire-tables": "^2.12",
        "sentry/sentry-laravel": "^3.3",
        "socialiteproviders/apple": "^5.4",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/linkedin": "^4.2",
        "socialiteproviders/medium": "^4.1",
        "spatie/laravel-permission": "^5.10",
        "talendor/laravel-stability-ai": "^0.3.0",
        "tinymce/tinymce": "^6.4",
        "wireui/wireui": "^1.17"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
